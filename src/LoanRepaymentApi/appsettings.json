{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "IpWhitelist": [
      "127.0.0.1",
      "192.168.0.0/16",
      "172.16.0.0/12",
      "10.0.0.0/8"
    ],
    "EndpointWhitelist": [
      "get:/health"
    ],
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "post:/ukstudentloans/calculate",
        "Period": "1m",
        "Limit": 60
      }
    ]
  }
}
